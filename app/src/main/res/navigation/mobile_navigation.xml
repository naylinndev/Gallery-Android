<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="dev.naylinn.gallery.ui.home.view.fragments.PhotoFragment"
        android:label="@string/title_photo"
        tools:layout="@layout/fragment_photo" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="dev.naylinn.gallery.ui.dashboard.DashboardFragment"
        android:label="@string/title_category"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="dev.naylinn.gallery.ui.notifications.NotificationsFragment"
        android:label="@string/title_favorite"
        tools:layout="@layout/fragment_notifications" />
</navigation>